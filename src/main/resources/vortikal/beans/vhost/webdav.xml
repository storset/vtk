<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  
  <bean id="webdavServiceAssertion" class="org.vortikal.web.service.AndAssertion">
    <property name="assertions">
      <list>
        <ref bean="webdavPortAssertion" />
        <ref bean="webdavHostNameAssertion" />
        <ref bean="webdavProtocolAssertion" />
      </list>
    </property>
  </bean>

  <bean id="webdav.davMountAcceptHeaderAssertion"
        class="org.vortikal.web.service.RequestHeaderRegexpAssertion">
    <property name="header" value="Accept" />
    <property name="pattern" value="application/davmount\+xml.*" />
  </bean>

  <bean id="webdav.davMountParameterAssertion"
        class="org.vortikal.web.service.RequestParameterAssertion">
    <property name="parameterName" value="vrtx" />
    <property name="parameterValue" value="davmount" />
  </bean>

  <bean id="webdavService" class="org.vortikal.web.service.ServiceImpl">
    <property name="order" value="-100" />
    <property name="assertions">
      <list>
        <ref bean="webdavServiceAssertion" />
      </list>
    </property>
    <property name="urlPostProcessors" ref="webdavServiceUrlPostProcessors" />
    <property name="authenticationChallenge" ref="${webdavAuthenticationChallenge}" />
    <property name="attributes">
      <map>
        <entry key="inhibit-caching" value="true" />
      </map>
    </property>
  </bean>
  
  <bean id="webdav.davMountService" class="org.vortikal.web.service.ServiceImpl">
    <property name="order" value="-100" />
    <property name="assertions">
      <list>
        <ref bean="webdav.davMountAcceptHeaderAssertion" />
        <ref bean="resourceInCollection" />
        <ref bean="webdav.davMountParameterAssertion" />
      </list>
    </property>
    <property name="authenticationChallenge" ref="${webdavAuthenticationChallenge}" />
    <property name="handler" ref="webdav.davMountHandler" />
  </bean>

  <bean id="webdav.davMountHandler" 
    class="org.vortikal.web.display.file.ResourceAwareParameterizableViewController">
    <property name="viewName" value="webdav.davMount" />
  </bean>

  <bean id="webdavServiceUrlPostProcessors" class="org.springframework.beans.factory.config.ListFactoryBean">
    <property name="sourceList">
      <list>
      </list>
    </property>
  </bean>

  <bean id="realWebdavClientService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="webdavService" />
    <property name="assertions">
      <list>
        <ref bean="emptyQueryStringAssertion" />
        <ref bean="isAuthenticatedAssertion" />
      </list>
    </property>
  </bean>

   <bean id="webdavProtocolAssertion"
         class="org.vortikal.web.service.RequestProtocolAssertion">
     <property name="protocol" value="${webdavProtocol}" />
     <property name="preferRequestProtocol" value="false" />
   </bean>

   <bean id="webdavPortAssertion"
         class="org.vortikal.web.service.RequestPortAssertion">
     <property name="port" value="${webdavPort}" />
   </bean>

   <bean id="webdavHostNameAssertion"
         class="org.vortikal.web.service.RequestHostNameAssertion">
     <property name="hostName" value="${webdavHostName}" />
   </bean>

   <bean id="emptyQueryStringAssertion"
         class="org.vortikal.web.service.RequestEmptyQueryStringAssertion" />

  <!-- DAV handler services: -->

  <bean id="webdavCollectionListingService" class="org.vortikal.web.service.ServiceImpl">
    <property name="order" value="-1" />
    <property name="parent" ref="webdavService" />
    <property name="assertions">
      <list>
        <bean class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="GET" />
        </bean>
        <ref bean="resourceInCollection" />
      </list>
    </property>
    <property name="handler" ref="webdavCollectionListingHandler" />
    <property name="attributes">
      <map>
        <entry key-ref="system.decoratorTemplateAttribute" value="webdav-collectionlisting.html" />
      </map>
    </property>
  </bean>

  <bean id="webdavCollectionListingHeadService" class="org.vortikal.web.service.ServiceImpl">
    <property name="order" value="-1" />
    <property name="parent" ref="webdavService" />
    <property name="assertions">
      <list>
        <bean class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="HEAD" />
        </bean>
        <ref bean="resourceInCollection" />
      </list>
    </property>
    <property name="handler" ref="webdavCollectionListingHandler" />
  </bean>

  <bean id="webdavOptionsService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="options" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="OPTIONS" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavOptionsHandler" />
  </bean>

  <bean id="webdavGetService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="get" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="GET" />
        </bean>
      </list>
    </property>
    <property name="handlerInterceptors">
      <list>
        <ref bean="webdavDisplayResourceHandlerInterceptor" />
      </list>
    </property>
    <property name="handler" ref="webdavGetHandler" />
  </bean>

  <bean id="webdavHeadService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="head" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="HEAD" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavHeadHandler" />
  </bean>

  <bean id="webdavPropfindService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="propfind" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="PROPFIND" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavPropfindHandler" />
  </bean>

  <bean id="webdavLockService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="lock" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="LOCK" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavLockHandler" />
  </bean>

  <bean id="webdavUnlockService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="unlock" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="UNLOCK" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavUnlockHandler" />
  </bean>

  <bean id="webdavPutService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="put" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="PUT" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavPutHandler" />
  </bean>

  <bean id="webdavMkColService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="MkCol" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="MKCOL" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavMkColHandler" />
  </bean>

  <bean id="webdavPostService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Post" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="POST" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavPutHandler" />
  </bean>

  <bean id="webdavCopyService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Copy" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="COPY" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavCopyHandler" />
  </bean>

  <bean id="webdavMoveService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Move" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="MOVE" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavMoveHandler" />
  </bean>

  <bean id="webdavDeleteService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Delete" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="DELETE" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavDeleteHandler" />
  </bean>

  <bean id="webdavProppatchService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Proppatch" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="PROPPATCH" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="webdavProppatchHandler" />
  </bean>

  <!-- The following are not implemented yet: -->

  <bean id="webdavSearchService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Search" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="SEARCH" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavReportService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Report" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="REPORT" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavACLService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="ACL" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="ACL" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavVersionControlService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="VersionControl" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="VERSION-CONTROL" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavCheckoutService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Checkout" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="CHECKOUT" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavUncheckoutService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Uncheckout" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="UNCHECKOUT" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavLabelService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Label" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="LABEL" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>

  <bean id="webdavUpdateService" class="org.vortikal.web.service.ServiceImpl">
    <property name="parent" ref="realWebdavClientService" />
    <property name="assertions">
      <list>
        <bean id="Update" class="org.vortikal.web.service.RequestMethodAssertion">
          <property name="method" value="UPDATE" />
        </bean>
      </list>
    </property>
    <property name="handler" ref="methodNotImplementedHandler" />
  </bean>
  
  <!-- WebDAV controllers -->

  <bean id="methodNotImplementedHandler" 
        class="org.vortikal.web.http.HttpMethodNotImplementedHandler"/>

  <bean id="webdavCollectionListingHandler"
        class="org.vortikal.web.display.file.ResourceAwareParameterizableViewController">
    <property name="viewName" value="webdavCollectionListingView" />
  </bean>

  <bean id="webdavOptionsHandler" class="org.vortikal.webdav.OptionsController" />

  <bean id="webdavGetHandler" class="org.vortikal.web.display.file.DisplayResourceController">
    <property name="viewName" value="webdavDisplayResource" />
    <property name="ignoreLastModified" value="true" />
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
  </bean>

  <bean id="webdavHeadHandler" class="org.vortikal.webdav.HeadController" />

  <bean id="webdavPropfindHandler" class="org.vortikal.webdav.PropfindController" />

  <bean id="webdavLockHandler" class="org.vortikal.webdav.LockController">
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
    <property name="deniedFileNames" ref="webdav.deniedFileNamePatterns" />
  </bean>

  <bean id="webdavUnlockHandler" class="org.vortikal.webdav.UnlockController">
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
  </bean>

  <bean id="webdavPutHandler" class="org.vortikal.webdav.PutController">
    <property name="maxUploadSize" value="${upload.maxFileSize}" />
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}"/>
    <property name="obeyClientCharacterEncoding" value="false" />
    <property name="deniedFileNames" ref="webdav.deniedFileNamePatterns" />
  </bean>

  <bean id="webdav.deniedFileNamePatterns" class="org.vortikal.context.CSVListFactoryBean">
    <property name="csvList" value="${webdav.deniedFiles}" />
    <property name="trim" value="true" />
  </bean>

  <bean id="webdavMkColHandler" class="org.vortikal.webdav.MkColController">
    <property name="deniedFileNames" ref="webdav.deniedFileNamePatterns" />
  </bean>

  <bean id="webdavCopyHandler" class="org.vortikal.webdav.CopyController">
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
  </bean>

  <bean id="webdavMoveHandler" class="org.vortikal.webdav.MoveController">
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
  </bean>

  <bean id="webdavDeleteHandler" class="org.vortikal.webdav.DeleteController">
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
  </bean>

  <bean id="webdavProppatchHandler" class="org.vortikal.webdav.ProppatchController">
    <property name="supportIfHeaders" value="${webdavSupportIfHeaders}" />
    <property name="webdavService" ref="webdavService"/>
  </bean>

  <!-- View resolver -->
  
  <bean id="webdavViewResolver" class="org.vortikal.web.decorating.MappingViewResolver">
    <property name="views">
      <map>
        <entry key="webdav.davMount" value-ref="webdav.davMountView" />
        <entry key="webdavDisplayResource" value-ref="webdavDisplayResourceView" />
        <entry key="OPTIONS" value-ref="OPTIONS" />
        <entry key="PROPFIND" value-ref="PROPFIND" />
        <entry key="HEAD" value-ref="HEAD" />
        <entry key="LOCK" value-ref="LOCK" />
        <entry key="PROPPATCH" value-ref="HTTP_STATUS_VIEW" />
        <entry key="MKCOL" value-ref="HTTP_MKCOL_STATUS_VIEW" />
        <entry key="MOVE" value-ref="HTTP_STATUS_VIEW" />
        <entry key="UNLOCK" value-ref="HTTP_STATUS_VIEW" />
        <entry key="COPY" value-ref="HTTP_STATUS_VIEW" />
        <entry key="PUT" value-ref="HTTP_STATUS_VIEW" />
        <entry key="POST" value-ref="HTTP_STATUS_VIEW" />
        <entry key="DELETE" value-ref="HTTP_STATUS_VIEW" />
        <entry key="HTTP_STATUS_VIEW" value-ref="HTTP_STATUS_VIEW" />
        <entry key="HTTP_STATUS_NOT_FOUND" value-ref="HTTP_STATUS_NOT_FOUND_VIEW" />
      </map>
    </property>
  </bean>

  <bean id="webdavCollectionListingViewResolver" parent="system.decoratingViewResolver">
    <property name="views">
      <map>
        <entry key="webdavCollectionListingHeadView" value-ref="webdavCollectionListingHeadView" />
        <entry key="webdavCollectionListingView" value-ref="webdavCollectionListingView" />
      </map>
    </property>
    <property name="referenceDataProviders">
      <list>
        <ref bean="manageLinkProvider" />
      </list>
    </property>
  </bean>

  <bean id="decorating.webdavNamespace" class="java.lang.String">
    <constructor-arg type="java.lang.String" value="webdav" />
  </bean>

  <bean id="webdav.cssComponent"
        class="org.vortikal.web.decorating.components.ViewRenderingDecoratorComponent">
    <property name="namespace" ref="decorating.webdavNamespace" />
    <property name="name" value="css" />
    <property name="description" value="Inserts the default WebDAV collection listing CSS stylesheets" />
    <property name="view" ref="webdav.cssView" />
    <property name="exposeMvcModel" value="true" />
  </bean>

  <bean id="webdav.cssView" parent="freemarkerView">
    <property name="url" value="system/css.ftl" />
    <property name="attributesMap">
      <map>
        <entry key="cssURLs">
          <list>
            <value>${themeBaseURL}/default.css</value>
            <value>${themeBaseURL}/webdav-collectionlisting.css</value>
            <!-- <value>http://www.uio.no/visuell-profil/css/uio.css</value> -->
          </list>
        </entry>
      </map>
    </property>
  </bean>

  <bean id="webdav.breadcrumbComponent"
        class="org.vortikal.web.decorating.components.ViewRenderingDecoratorComponent">
    <property name="namespace" ref="decorating.webdavNamespace" />
    <property name="name" value="breadcrumb" />
    <property name="description" value="Displays the WebDAV breadcrumb trail" />
    <property name="view" ref="webdav.breadcrumbView" />
  </bean>

  <bean id="webdav.breadcrumbView" parent="freemarkerView">
    <property name="url" value="layouts/breadcrumb.ftl" />
    <property name="referenceDataProviders">
      <list>
        <bean class="org.vortikal.web.referencedata.provider.BreadCrumbProvider">
          <property name="breadcrumbName" value="breadcrumb" />
          <property name="service" ref="webdavService"/>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="webdav.infoMessageComponent"
        class="org.vortikal.web.decorating.components.ViewRenderingDecoratorComponent">
    <property name="namespace" ref="decorating.webdavNamespace" />
    <property name="name" value="info-message" />
    <property name="description" value="Displays the WebDAV info message" />
    <property name="view" ref="webdav.infoMessageView" />
  </bean>

  <bean id="webdav.infoMessageView" parent="freemarkerView">
    <property name="url" value="pages/webdav-info.ftl" />
  </bean>

  <!-- View definitions -->

  <bean id="webdavCollectionListingView" parent="freemarkerView">
    <property name="url" value="pages/webdav-collectionlisting.ftl" />
    <property name="referenceDataProviders">
      <list>
        <bean class="org.vortikal.web.referencedata.provider.ResourceServiceURLProvider">
          <property name="service" ref="webdav.davMountService" />
          <property name="modelName" value="davMountService" />
          <property name="urlName" value="url" />
          <property name="matchAssertions" value="true" />
        </bean>
        <ref bean="resourceContextProvider" />
        <ref bean="webdavBreadCrumbProvider" />
        <ref bean="webdavCollectionListingProvider" />
      </list>
    </property>
  </bean>

  <bean id="webdavCollectionListingHeadView" parent="webdavCollectionListingView" />

  <bean id="webdavDisplayResourceHandlerInterceptor"
        class="org.vortikal.web.interceptors.HeaderControlHandlerInterceptor">
    <property name="includeLastModifiedHeader" value="false" />
    <property name="includeEtagHeader" value="${webdavSupportETags}" />
    <!-- Because of a bug in Resin (does not distinguish between GET
         and PUT?), set a bunch of cache-preventing headers here: -->
    <property name="staticHeaders">
      <map>
        <entry key="Cache-Control" value="no-cache, no-store, must-revalidate, max-age=0" />
        <entry key="Expires" value="0" />
        <entry key="Pragma" value="no-cache" />
      </map>
    </property>
  </bean>

  <bean id="webdav.davMountView" parent="freemarkerView">
    <property name="contentType" value="application/davmount+xml" />
    <property name="url" value="pages/webdav-davmount.ftl" />
    <property name="referenceDataProviders">
      <list>
        <ref bean="resourceContextProvider" />
        <bean class="org.vortikal.web.referencedata.provider.ResourceServiceURLProvider">
          <property name="service" ref="webdavService" />
          <property name="modelName" value="webdavService" />
          <property name="urlName" value="url" />
        </bean>
      </list>
    </property>
  </bean>

  <bean id="webdavDisplayResourceView" class="org.vortikal.web.view.DisplayResourceView" />

  <bean id="HTTP_STATUS_VIEW" class="org.vortikal.web.view.SimpleHttpStatusView"/>

  <bean id="HTTP_MKCOL_STATUS_VIEW" class="org.vortikal.web.view.SimpleHttpStatusView">
    <property name="contentType" value="text/html;charset=utf-8" />
    <property name="body">
      <value>
        &lt;?xml version="1.0" encoding="utf-8"?&gt;
        &lt;html&gt;&lt;head&gt;
        &lt;title&gt;Created&lt;/title&gt;
        &lt;/head&gt;
        &lt;body&gt;Document created&lt;/body&gt;
        &lt;/html&gt;
      </value>
    </property>
  </bean>

  <bean id="HTTP_STATUS_NOT_FOUND_VIEW"
        class="org.vortikal.web.view.SimpleHttpStatusView">
    <property name="attributesMap">
      <map>
        <entry key="httpStatusCode">
          <bean class="java.lang.Integer">
            <constructor-arg><value>404</value></constructor-arg>
          </bean>
        </entry>
      </map>  
    </property>
  </bean>  

  <bean id="OPTIONS" class="org.vortikal.webdav.OptionsView"/>

  <bean id="PROPFIND" class="org.vortikal.webdav.PropfindView">
    <property name="webdavService" ref="webdavService"/>
    <property name="collectionContentType" value="${webdav.collectionContentType}" />
  </bean>

  <bean id="HEAD" class="org.vortikal.web.view.DisplayResourceView" />

  <bean id="LOCK" class="org.vortikal.webdav.LockView"/>

  <bean id="webdavBreadCrumbProvider"
        class="org.vortikal.web.referencedata.provider.BreadCrumbProvider">
    <property name="service" ref="webdavService"/>
